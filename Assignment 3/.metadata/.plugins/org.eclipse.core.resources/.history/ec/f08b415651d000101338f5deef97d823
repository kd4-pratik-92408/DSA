import java.util.Stack;

public class Tester {
	public static int calculate(int op1, String opr, int op2) {
		switch(opr) {
		case "+": return op1 + op2;
		case "-": return op1 - op2;
		case "/": return op1 / op2;
		case "*": return op1 * op2;
		case "%": return op1 % op2;
		case "$": return (int)Math.pow(op1, op2);
		}
		return 0;
	}

	public static int postfixEval(String postfix ) {
		Stack<Integer> st=  new Stack<>();
		String[] str= postfix.split(" ");
//		String opr=null;
		for(String sing: str) {
			if(sing.equals("/") || sing.equals("*") || sing.equals("+") || sing.equals("-") || sing.equals("$") || sing.equals("%")    ){
			
				int op2=st.pop();
				int op1= st.pop();
				int res=calculate(op1, sing, op2);
			
		}else {
			st.push(Integer.parseInt(sing));
		}
			
			
		}
		
	}
	public static void main(String[] args) {
		
	}

}
