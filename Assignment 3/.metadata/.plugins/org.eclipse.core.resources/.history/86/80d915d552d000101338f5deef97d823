import java.util.Stack;

public class Tester {
	public static int calculate(int op1, String opr, int op2) {
		switch(opr) {
		case "+": return op1 + op2;
		case "-": return op1 - op2;
		case "/": return op1 / op2;
		case "*": return op1 * op2;
		case "%": return op1 % op2;
		case "$": return (int)Math.pow(op1, op2);
		}
		return 0;
	}

	public static int postfixEval(String postfix ) {
		Stack<Integer> st=  new Stack<>();
		String[] str= postfix.split(" ");
//		String opr=null;
		for(String sing: str) {
			if(sing.equals("/") || sing.equals("*") || sing.equals("+") || sing.equals("-") || sing.equals("$") || sing.equals("%")    ){
			
				int op2=st.pop();
				int op1= st.pop();
				int res=calculate(op1, sing, op2);
				st.push(res);
			
		}else {
			st.push(Integer.parseInt(sing));
		}
			
			
		}
		if(!st.isEmpty()) {
			return st.pop();
		}
		return 0;
	}
	
	
	public static int prefixEval(String postfix ) {
		Stack<Integer> st=  new Stack<>();
		String[] str= postfix.split(" ");
//		String opr=null;
		for(int i=str.length;i>=0;i--) {
			if(str[i].equals("/") || str[i].equals("*") || str[i].equals("+") || sing.equals("-") || sing.equals("$") || sing.equals("%")    ){
			
				int op2=st.pop();
				int op1= st.pop();
				int res=calculate(op1, sing, op2);
				st.push(res);
			
		}else {
			st.push(Integer.parseInt(sing));
		}
			
			
		}
		if(!st.isEmpty()) {
			return st.pop();
		}
		return 0;
	}
	public static void main(String[] args) {
			
		String postfix = "4 50 6 * 30 / + 95 + 70 -";
		
		System.out.println("Postfix : " + postfix);
		int result = postfixEval(postfix);
		System.out.println("Result : " + result);
	}

}
